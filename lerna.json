{
  "packages": [
    "packages/@cssUI/*"
  ],
  "useWorkspaces":true,
  "npmClient":"npm",
  "version": "0.0.0",
  "command":{
    "publish":{
      "ignoreChanges":[
        "ignored-file",
        "*.md"
      ],
      "conventionalCommits":true,
      "message":"chore(release): publish %s"
    },
    "bootstrap":{
      "npmClientArgs":["--no-package-lock"]
    }
  }
}
